
#include<bits/stdc++.h>
using namespace std;
#define lli long long int
#define ulli unsigned long long int
#define MOD 1000000007
string solve(int *arr,int n,int h,int x){
    for(int i=0;i<n;i++){
        if(arr[i]+x >= h){
            return "YES";
        }
    }

    return "NO";
}


int main()
{
            
                int n,h,x;
                cin>>n>>h>>x;
                int arr[n];
                for(int i=0;i<n;i++){
                    cin>>arr[i];
                }
                cout<<solve(arr,n,h,x)<<endl;
            

}



